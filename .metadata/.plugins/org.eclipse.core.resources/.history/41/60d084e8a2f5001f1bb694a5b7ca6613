package logic;

public class reverceString2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str = "anagram";
		String str2 = "";
		for(int i=0;i<str.length();i++) {
			str2=str.indexOf(i)+str2;
		}
		if(str.equals(str2))
			System.out.println(true);
		else
			System.out.println(false);
	}

}
